{
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "no-mixed-spaces-and-tabs": 1,
    "import/default": 0,
    "import/no-duplicates": 0
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ]
}